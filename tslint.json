{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "max-classes-per-file": false,
        "class-name": false,
        "member-access": false,
        "interface-over-type-literal": false,
        "interface-name": false,
        "array-type": [true, "array"],
        "curly": false
    },
    "rulesDirectory": []
}
